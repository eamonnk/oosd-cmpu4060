import random
import string

upper_do_work_rand_int=min(random.randint(1, 10), 6)
print(upper_do_work_rand_int)
        
        #use that random number to generate that number if random letters 
upper_letters = string.ascii_lowercase
print(upper_letters)

# "clipped" means limiting or constraining the random number to a specific maximum valueâ€”the length of the queue. This prevents trying to # process more elements than are available in the queue.
        # use the random choices method specifying the letters we created and k defines the length of the list
        # which is our random num between 1 and 10 which we created earlier 
upper_do_work_rand_letters=random.choices(upper_letters, k=upper_do_work_rand_int)
    for upper_letter_obj in self.upper_do_work_rand_letters:
